{
  "manifest_version": 3,
  "name": "Form Autofill AI",
  "version": "1.0.0",
  "description": "Auto-fill forms with AI and saved profiles",
  "permissions": ["storage", "scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Form Autofill AI"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}